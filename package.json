{
  "name": "youtube-sound-track-download-extension",
  "version": "1.0.0",
  "description": "Chrome extension for downloading youtube soundtracks",
  "main": "extension.ts",
  "dependencies": {
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.75",
    "@types/react": "16.7.17",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "clean-webpack-plugin": "1.0.0",
    "commitizen": "3.0.5",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.0.1",
    "cz-customizable": "5.3.0",
    "husky": "1.2.1",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-loader": "5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "3.2.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2"
  },
  "scripts": {
    "build": "webpack --config=config/webpack/webpack.config.js --mode=production",
    "develop": "webpack --config=config/webpack/webpack.config.js --mode=development --watch --info-verbosity verbose",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "Chrome",
    "Youtube",
    "Extension",
    "JavaScript"
  ],
  "author": "Tautvydas Derzinskas",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimDogs/youtube-sound-track-download-extension.git"
  },
  "license": "MIT",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/cz-customizable/base.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
