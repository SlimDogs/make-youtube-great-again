{
  "name": "youtube-sound-track-download-extension",
  "version": "1.0.0",
  "description": "Chrome extension for downloading youtube soundtracks",
  "main": "extension.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/chrome": "0.0.74",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^3.0.4",
    "copy-webpack-plugin": "^4.5.4",
    "cz-customizable": "^5.2.0",
    "husky": "^1.1.2",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "css-loader": "^1.0.0",
    "ts-loader": "^5.2.2",
    "tslint": "5.9.1",
    "tslint-loader": "3.5.3",
    "typescript": "^3.1.3",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "build": "webpack --config=config/webpack/webpack.config.js --mode=production",
    "develop": "webpack --config=config/webpack/webpack.config.js --mode=development --watch --info-verbosity verbose",
    "lint": "tslint src/**/*.ts"
  },
  "keywords": [
    "Chrome",
    "Youtube",
    "Extension",
    "JavaScript"
  ],
  "author": "Tautvydas Derzinskas",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimDogs/youtube-sound-track-download-extension.git"
  },
  "license": "MIT",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/cz-customizable/base.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
